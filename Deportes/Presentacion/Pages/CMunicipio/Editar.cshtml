@page
@model Presentacion.Pages.CMunicipio.EditModel
@{
    ViewData["Title"] = "Editar";
}

<h1>Editar Municipio</h1>
<h2 class="">@ViewData["Error"]</h2>

<div class="form-horizontal">
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <input type="hidden"  asp-page="ObjMunicipio.ID"/>
                <div class="form-group">
                    <label asp-for="ObjMunicipio.Id" class="control-label" hidden></label>
                    <input asp-for="ObjMunicipio.Id" class="control-label" hidden/>
                    <label asp-for="ObjMunicipio.Nombre" class="control-label"></label>
                    <input asp-for="ObjMunicipio.Nombre" class="control-label" />
                </div>
                <div class="form-group">
                    <input type="submit" value="MODIFICAR" class="btn btn-success"/>
                </div>
            </form>
        </div>
    </div>
    <div>
        <a asp-page="./Index">Ir al Inicio</a>
    </div>
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}




