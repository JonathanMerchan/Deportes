@page
@model Presentacion.Pages.Cmunicipio.CrearModel
@{
    ViewData["Title"] = "Crear";
}

<h1>Crear Municipio</h1>
<h2 class="">@ViewData["Error"]</h2>

<div class="form-horizontal">
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <div class="form-group">
                    <label asp-for="Municipio.Nombre"></label>
                    <input asp-for="Municipio.Nombre"/>
                </div>
                <div class="form-group">
                    <input type="submit" value="CREAR"/>
                </div>
            </form>
        </div>
    </div>
    <div>
        <a asp-page="./Index">Ir al Inicio</a>
    </div>
</div>

@section Scripts{ 
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}
